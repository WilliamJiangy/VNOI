#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;

#define testcase() {ll t; cin >> t; while(t--) {solve();}}
#define all(x) (x).begin(), (x).end()
#define sz(x) ((ll)(x).size())
#define pb push_back
#define ar array
#define bug() cout << "######PLEASE_NO_BUG######\n"

void solve() {
	ll A[3];
	while(cin >> A[0]) {
		cin >> A[1] >> A[2];
		bool ok = false;
		for(int mask = 0; mask < 1 << 3; mask++) {
			if(__builtin_popcount(mask) == 1) {
				int res = 0;
				for(int i = 0; i < 3; i++) {
					if(mask >> i & 1) {
						res -= A[i];
					}
					else res += A[i];
				}
				if(!res) {
					puts("yes");
					ok = true;
					break;
				}
			}
		}
		if(!ok) puts("no");
	}
}

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(nullptr);
//	testcase();
	solve();
	return 0;
}
