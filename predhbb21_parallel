#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;

#define testcase() {ll t; cin >> t; while(t--) {solve();}}
#define all(x) (x).begin(), (x).end()
#define sz(x) ((ll)(x).size())
#define pb push_back
#define ar array
#define bug() cout << "######PLEASE_NO_BUG######\n"

void solve() {
	int a[12];
	bool is = true;
	while(is) {
		int cnt = 0;
		for(int i = 0; i < 12; i++) {
			cin >> a[i];
			if(!a[i]) cnt++;
		}
		if(cnt == 12) break;
		sort(a, a + 12);
		bool ok = true;
		for(int i = 1; i < 4; i++) {
			if(a[i] != a[i - 1]) ok = false;
		}
		for(int i = 5; i < 8; i++) {
			if(a[i] != a[i - 1]) ok = false;
		}
		for(int i = 9; i < 12; i++) {
			if(a[i] != a[i - 1]) ok = false;
		}
		if(ok) puts("yes");
		else puts("no");
	}
}

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(nullptr);
//	testcase();
	solve();
	return 0;
}
