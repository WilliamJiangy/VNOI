#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;

#define testcase() {ll t; cin >> t; while(t--) {solve();}}
#define all(x) (x).begin(), (x).end()
#define sz(x) ((ll)(x).size())
#define pb push_back
#define ar array
#define bug() cout << "######PLEASE_NO_BUG######\n"

void solve() {
	int n;
	cin >> n;
	for(int i = 0, a, b; i < n; i++) {
		cin >> a >> b;
		if((a <= 1 && b <= 2) || (a <= 2 && b <= 1)) puts("Yes");
		else puts("No");
	}
}

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(nullptr);
//	testcase();
	solve();
	return 0;
}
